<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}" dir="{{ settings.text_direction }}">
<head>
  {% if request.page_type == 'product' and product.id == 7662922104895 %}
<meta http-equiv="refresh" content="0;URL={{ routes.root_url }}">
    {% endif %}
